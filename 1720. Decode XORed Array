class Solution {
public:
    vector<int> decode(vector<int>& a, int first) 
    {   
        int n = a.size();
        vector<int>v;   
        v.push_back(first);
        int j = 0;
        for(int i=0;i<n;i++)
        {
            int x = v[j];
            x = x^a[i];
            v.push_back(x);
            j++;
        }
        return v;
    }
};
